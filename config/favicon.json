
{
    "master_picture": "app/assets/images/master_picture.png",
    "favicon_design": {
        "ios": {
            "picture_aspect": "no_change",
            "assets": {
                "ios6_and_prior_icons": true,
                "ios7_and_later_icons": true,
                "precomposed_icons": true,
                "declare_only_default_icon": true
            }
        },
        "desktop_browser": [

        ],
        "windows": {
            "picture_aspect": "no_change",
            "background_color": "#da532c",
            "on_conflict": "override",
            "assets": {
                "windows_80_ie_10_tile": true,
                "windows_10_ie_11_edge_tiles": {
                    "small": true,
                    "medium": true,
                    "big": true,
                    "rectangle": true
                }
            }
        },
        "android_chrome": {
            "picture_aspect": "no_change",
            "theme_color": "#ffffff",
            "manifest": {
                "name": "QRDIY",
                "display": "standalone",
                "orientation": "not_set",
                "on_conflict": "override",
                "declared": true
            },
            "assets": {
                "legacy_icon": true,
                "low_resolution_icons": true
            }
        },
        "safari_pinned_tab": {
            "picture_aspect": "black_and_white",
            "threshold": 75,
            "theme_color": "#5bbad5"
        }
    },
    "settings": {
        "scaling_algorithm": "Mitchell",
        "error_on_image_too_small": true
    }
}
